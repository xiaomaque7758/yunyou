\">\r\n        <div class=\"mui-card-content-inner\">\r\n          <span class='old'>市场价：1000</span>\r\n          <span class='new'>销售价：100</span>\r\n          <p>购买数量</p>\r\n          <div class=\"mui-numbox\">\r\n\t\t\t\t\t<button class=\"mui-btn mui-btn-numbox-minus\" type=\"button\" @click=\"goodSub\">-</button>\r\n\t\t\t\t\t<input class=\"mui-input-numbox\" type=\"number\" value='1' v-model='val'/>\r\n\t\t\t\t\t<button class=\"mui-btn mui-btn-numbox-plus\" type=\"button\"\r\n          @click=\"goodAdd\">+</button>\r\n\t\t\t\t</div>\r\n        </div>\r\n      </div>\r\n      <div class=\"mui-card-footer\">\r\n          <mt-button type='danger' size='small'>立即购买</mt-button>\r\n          <mt-button type='primary' size='small' @click=\"addCart\">加入购物车</mt-button>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\n  import {Toast} from \"mint-ui\"\r\n  //引入轮播图子组件\r\n  import swipe from '../sub/swipe.vue'\r\n  export default{\r\n    created(){  \r\n      this.getImage();\r\n    },\r\n    methods:{\r\n      goodSub(){\r\n        if(this.val>1){\r\n           this.val--;\r\n        }\r\n       \r\n      },\r\n      goodAdd(){\r\n        if(this.val<999){\r\n           this.val++;\r\n        }\r\n      },\r\n      addCart(){\r\n        var pid=this.$route.params.id;\r\n        var count=this.val;\r\n        count=parseInt(count);\r\n        var uid=3;\r\n        var url=`http://127.0.0.1:3000/addCart?pid=${pid}&count=${count}&uid=${uid}`;\r\n        this.axios.get(url).then(res=>{\r\n          var obj=res.data;\r\n          if(obj.code=='1'){\r\n            this.$store.commit(\"increment\",count);\r\n            Toast(obj.msg);\r\n          }\r\n          else{\r\n            Toast(obj.msg); \r\n          }\r\n        })\r\n      },\r\n      getImage(){\r\n        var url = \"http://127.0.0.1:3000/imagelist\"\r\n        this.axios.get(url).then(res=>{\r\n          //console.log(res);\r\n          this.list=res.data;\r\n        })\r\n      }\r\n    },\r\n    data(){\r\n      return{\r\n        nid:this.$route.params.id,\r\n        list:[],\r\n        val:1\r\n      }\r\n    },\r\n    components:{\r\n      \"swiper-box\":swipe\r\n    }\r\n  }\r\n</script>\r\n<style scoped>\r\n  .new{\r\n    font-size:16px;\r\n    color:red;\r\n    font-weight:bold;\r\n  }\r\n   .old{\r\n    font-size:12px;\r\n    text-decoration:line-through;\r\n  }\r\n  .num{\r\n    padding:0 10px;\r\n    \r\n  }\r\n</style>"],"sourceRoot":"src\\components\\good"}]}